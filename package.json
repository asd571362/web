{
  "name": "infosat",
  "description": "Infosat",
  "private": false,
  "scripts": {
    "dev": "nuxt dev --port 6636",
    "build": "nuxt build",
    "start": "node .output/server/index.mjs",
    "generate": "nuxi generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "serve": "esno .output/server/index.mjs",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix",
    "copy_prod_env": "yarn rm_env && shx cp .env.production .env",
    "copy_preview_env": "yarn rm_env && shx cp .env.preview .env",
    "rm_env": "shx rm -f .env",
    "sync_api": "yarn ts-node tools/sync.ts",
    "generate_api": "yarn openapi-typescript tools/swagger.json --output types/swagger.ts && yarn ts-node tools/generate.ts && yarn lintfix"
  },
  "devDependencies": {
    "@nuxt/devtools": "^0.4.0",
    "@nuxt/types": "^2.16.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/i18n": "^8.0.0-beta.10",
    "@pinia-plugin-persistedstate/nuxt": "^1.1.1",
    "@pinia/nuxt": "^0.4.5",
    "@vite-pwa/nuxt": "^0.1.0",
    "@vueuse/nuxt": "^9.6.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^9.0.0",
    "naive-ui": "^2.34.3",
    "nuxt": "^3.0.0",
    "openapi-typescript": "5.4.1",
    "path": "^0.12.7",
    "prettier": "^2.8.4",
    "replace-in-file": "^6.3.5",
    "sass": "^1.58.3",
    "stylelint": "^15.2.0",
    "ts-morph": "^18.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.15.1",
    "vite-plugin-stylelint": "^4.2.0"
  },
  "dependencies": {
    "@css-render/vue3-ssr": "^0.15.12",
    "@unisat/ord-utils": "^0.3.2",
    "dayjs": "^1.11.9",
    "ethers": "^6.2.2",
    "pinia": "^2.0.26",
    "vue-final-modal": "^4.1.4"
  }
}
